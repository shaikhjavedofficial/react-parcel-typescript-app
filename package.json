{
  "name": "React Parcel App",
  "version": "0.1.0",
  "private": true,
  "source": "public/index.html",
  "keywords": [],
  "author": "Shaikh Javed & Saniya Shaikh",
  "license": "ISC",
  "description": "React parcel App with typescript and Redux",
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@mui/material": "^5.16.7",
    "@parcel/optimizer-htmlnano": "^2.12.0",
    "@parcel/transformer-css": "^2.12.0",
    "@parcel/watcher": "^2.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.4",
    "lightningcss": "^1.26.0",
    "notistack": "^3.0.1",
    "parcel": "^2.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "remixicon": "^4.3.0",
    "svgo": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "npm run clear && parcel public/index.html --dist-dir build --open chrome --port 3000",
    "build": "npm run clear && parcel build public/index.html --dist-dir build",
    "clear": "rm -rf build .parcel-cache && npm run bundle",
    "bundle": "mkdir build && cp public/robots.txt build/",
    "test": "jest --clearMocks --no-cache --watchAll --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@parcel/compressor-brotli": "^2.12.0",
    "@parcel/compressor-gzip": "^2.12.0",
    "@types/css-modules": "^1.0.5",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "process": "^0.11.10",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2"
  },
  "parcel-namer-rewrite": {
    "profiles": {
      "development": {
        "disable": true
      },
      "production": {
        "chain": "@parcel/namer-default",
        "rules": {
          "(.*).css$": "styles/$1{.hash}.css",
          "public(.*).json": "manifest{.hash}.json",
          "public(.*).xml": "browserconfig{.hash}.xml",
          "(.*).js$": "scripts/$1{.hash}.js",
          "(.*).png$": "assets/images/$1{.hash}.png",
          "(.*).webp$": "assets/images/$1{.hash}.webp",
          "(.*).jpg$": "assets/images/$1{.hash}.jpg",
          "(.*).jpeg$": "assets/images/$1{.hash}.jpeg",
          "(.*).svg$": "assets/images/$1{.hash}.svg",
          "(.*).gif$": "assets/images/$1{.hash}.gif",
          "(.*).avif$": "assets/images/$1{.hash}.avif",
          "version.html": "version.json"
        }
      }
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "dashedIdents": true
    },
    "drafts": {
      "nesting": true,
      "customMedia": true
    }
  }
}
